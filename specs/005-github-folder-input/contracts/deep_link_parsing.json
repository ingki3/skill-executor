{
  "endpoints": [
    {
      "path": "/api/skills/parse-url",
      "method": "GET",
      "description": "Parse a GitHub URL to detect if it is a root repository or a deep link to a skill folder.",
      "query_params": {
        "url": "string"
      },
      "response": {
        "is_github": "boolean",
        "is_deep_link": "boolean",
        "repo_url": "string",
        "branch": "string | null",
        "sub_path": "string | null"
      }
    }
  ]
}
